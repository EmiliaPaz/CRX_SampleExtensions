{
  "name": "Green background - All urls",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page green in all urls",
  "permissions": [
    "scripting",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
